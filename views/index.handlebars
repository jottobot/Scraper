<div class="container">

  <div class="row" id="scrapedarticles">
    <div class="col-8" id="articles"></div>
    {{#each article}}
    <div class="align-self-end text-right">
      <button class="btn btn-outline-danger m-2 btn-save" data-id="{{this._id}}">Save Article</button>
    </div>
    {{/each}}
    <div class="col-4" id="notes"></div>
  </div>

</div>